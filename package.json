{
  "name": "hardly-an-app",
  "version": "2.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "build": "enoki content && browserify src/index.js -t [ envify --NODE_ENV production ] -t [ sheetify -u sheetify-cssnext ] -p [ css-extract -o bundles/bundle.css ] -t babelify -o bundles/bundle.js",
    "start": "enoki content --watch & budo src/index.js --dir . -s bundles/bundle.js -P -- -t sheetify -p [ css-extract -o bundles/bundle.css ]",
    "dev": "enoki content --watch & watchify src/index.js -t sheetify -p [ css-extract -o bundles/bundle.css ]",
    "serve": "python -m SimpleHTTPServer 8080",
    "test": "standard"
  },
  "author": "Jon-Kyle Mohr",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.49",
    "@babel/plugin-transform-async-to-generator": "^7.0.0-beta.49",
    "@babel/preset-env": "^7.0.0-beta.49",
    "babelify": "^9.0.0",
    "browserify": "^16.2.2",
    "css-extract": "^1.3.0",
    "envify": "^4.1.0",
    "lilcss": "0.0.0",
    "sheetify": "^7.3.2",
    "sheetify-cssnext": "^2.0.0",
    "standard": "^11.0.1",
    "tape": "^4.6.3",
    "tinyify": "^2.4.1",
    "uglify-js": "^3.3.28",
    "watchify": "^3.11.0",
    "yo-yoify": "^4.3.0"
  },
  "dependencies": {
    "choo": "^6.11.0",
    "clone-deep": "^0.2.4",
    "dayjs": "^1.6.4",
    "deep-equal": "^1.0.1",
    "enoki": "^2.2.0-next4",
    "fastclick": "^1.0.6",
    "gr8": "^3.1.5",
    "install": "^0.10.1",
    "lodash.throttle": "^4.1.1",
    "marked": "^0.4.0",
    "nanobus": "^4.3.3",
    "nanoreset": "^2.0.0",
    "normalize-url": "^1.6.1",
    "object-values": "^1.0.0",
    "plyr": "^3.3.9",
    "recsst": "^1.1.2",
    "simple-color-picker": "^0.1.2",
    "tags-input": "^1.1.1",
    "tinycolor2": "^1.4.1",
    "uuid": "^3.0.1",
    "valid-url": "^1.0.9",
    "webfontloader": "^1.6.26",
    "xtend": "^4.0.1"
  }
}
